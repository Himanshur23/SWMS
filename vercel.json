{
  "builds": [
    {
      "src": "build-command.sh",
      "use": "@vercel/static-build",
      "config": {
        "maxLambdaSize": "75mb"
      }
    }
  ],
  "version": 2,
  "env": {
    "RUSTUP_HOME": "/vercel/.rustup",
    "CARGO_HOME": "/vercel/.cargo"
  },
  "functions": {
    "api": {
      "memory": 1024
    }
  }
}