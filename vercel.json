{
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "10mb"
      }
    },
    {
      "src": "./",
      "use": "@vercel/static",
      "config": {
        "trailingSlash": true
      }
    },
    {
      "use": "@vercel/build-utils",
      "inputs": {
        "command": "curl https://sh.rustup.rs -sSf | sh -s -- -y && $HOME/.cargo/bin/cargo install --version=0.1.8 cbindgen"
      }
    }
  ]
}